
{
    "name": "datand/spotify-web-api",
    "description": "PHP library for the Spotify Web API",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "guzzle/guzzle": "6.*",
        "league/oauth2-server": "~2.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^2.5.8",
        "league/factory-muffin": "^2.1",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Datand\\Spotify\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "preferred-install": {
            "*": "dist"
        }
    }
}
