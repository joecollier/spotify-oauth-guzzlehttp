
{
    "name": "datand/spotify-web-api",
    "description": "PHP library for the Spotify Web API",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "6.*",
        "league/oauth2-client": "~1.4",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "doctrine/collections": "1.*"
    },
    "require-dev": {
        "kahlan/kahlan": "^2.5.8",
        "league/factory-muffin": "^2.1",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Datand\\Spotify\\": "src/",
            "Datand\\Spotify\\Api\\": "src/Api/",
            "Audeio\\Spotify\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "preferred-install": {
            "*": "dist"
        }
    }
}
